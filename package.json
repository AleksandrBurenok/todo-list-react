{
  "name": "todo-list-react",
  "version": "1.0.0",
  "description": "Simple Todo List",
  "main": "index.js",
  "scripts": {
    "setEnv:dev": "cross-env NODE_ENV=development",
    "setEnv:prod": "cross-env NODE_ENV=production",
    "serve": "npx webpack-dev-server --hot --inline --open --color --config ./config/webpack.dev.js",
    "lint:js": "npx eslint . --ext .js,.jsx --fix",
    "lint:css": "npx stylelint **/*.scss --fix",
    "lint": "yarn lint:js || yarn lint:css",
    "start": "yarn setEnv:dev yarn serve",
    "build": "yarn setEnv:prod npx webpack --config ./config/webpack.prod.js",
    "test": "npx jest",
    "styleguide": "npx styleguidist server"
  },
  "repository": "git@github.com:IgorBondar/todo-list-react.git",
  "author": "Ihor Bondar <ihor.bondar92@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "jest-enzyme": "^6.0.0",
    "node-sass": "^4.8.3",
    "postcss-loader": "^2.1.3",
    "react-styleguidist": "^7.0.4",
    "sass-loader": "^6.0.7",
    "sass-resources-loader": "^1.3.3",
    "style-loader": "^0.20.3",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "mobx": "^4.1.1",
    "mobx-react": "^5.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router-dom": "^4.2.2"
  }
}
